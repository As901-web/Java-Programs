public class ReverseString 
{
    public static void main(String args[])
    {
        String str="solution";
        int length=str.length();
        String rev=" ";
        for(int i=length-1;i>=0;i--)
        {
            rev=rev+str.charAt(i);
        }
        System.out.println("Reverse string of "+str+ " is in Uppercase  " +rev.toUpperCase());
    }
}
